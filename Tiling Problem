public class TillingProblem {
    public static int func(int n){
        if(n==1 || n==0){
            return 1;
        }

        int fnm1=func(n-1);
        int fnm2=func(n-2);

        return fnm1+fnm2;
    }
    public static void main(String[] args) {
        // 2xn  - -- - - 2x1  
        int n=8;
        System.out.println(func(n));
    }
}
