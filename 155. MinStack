class MinStack {

    Stack<Integer> st=new Stack<>();
    Stack<Integer> min_val=new Stack<>();
    
    public void push(int val) {
        if(min_val.isEmpty() || min_val.peek()>=val){
            min_val.push(val);
        }

        st.push(val);
    }
    
    public void pop() {
        if(min_val.peek().equals(st.peek())){
            min_val.pop();
        }

        st.pop();
    }
    
    public int top() {
        return st.peek();
    }
    
    public int getMin() {
        return min_val.peek();
    }
}

/**
 * Your MinStack object will be instantiated and called as such:
 * MinStack obj = new MinStack();
 * obj.push(val);
 * obj.pop();
 * int param_3 = obj.top();
 * int param_4 = obj.getMin();
 */
