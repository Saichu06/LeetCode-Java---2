class Solution {
    public int[] nextGreaterElements(int[] nums) {
        int n=nums.length;
        Stack<Integer> st=new Stack<>();
        int[] output=new int[n];
        Arrays.fill(output,-1);

        for(int i=0;i<n*2;i++){
            while(!st.isEmpty()  && nums[st.peek()]<nums[i%n]){
                output[st.pop()]=nums[i%n];
            }
            if(i<n) st.push(i);
        }   
        return output;
    }
}
