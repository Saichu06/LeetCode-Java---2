class Solution {
    public String removeDuplicates(String s) {
        Stack<Character> st=new Stack<>();
        int n=s.length();
        for(int i=0;i<n;i++){
            char ch=s.charAt(i);
            if(st.size()>0 && st.peek()==ch){
                st.pop();
            }
            else{
                st.push(ch);
            }
        }

        StringBuilder sc=new StringBuilder("");
        while(!st.isEmpty()){
            sc.append(st.pop());
        }

        return sc.reverse().toString();
    }
}
