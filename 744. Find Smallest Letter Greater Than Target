class Solution {
    public char nextGreatestLetter(char[] letters, char target) {
        char a='{';
        for(int i=0;i<letters.length;i++){
            if(letters[i]>target && letters[i]<a){
                a=letters[i];
            }
        }

        if(a=='{'){
            return letters[0];
        }else{
            return a;
        }
    }
}
